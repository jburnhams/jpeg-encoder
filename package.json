{
  "name": "jpeg-encoder-wasm",
  "version": "0.1.0",
  "description": "WebAssembly JPEG encoder",
  "main": "pkg/index.js",
  "module": "pkg/index.js",
  "types": "pkg/index.d.ts",
  "files": [
    "pkg"
  ],
  "type": "module",
  "scripts": {
    "build": "cargo build --manifest-path wasm-bindings/Cargo.toml --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/jpeg_encoder_wasm.wasm --out-dir pkg --target web --typescript --out-name jpeg_encoder",
    "prepare:docs": "mkdir docs/dist && cp -r pkg/* docs/dist/",
    "test": "node test/integration.test.js",
    "test:browser": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js",
    "test:all": "npm test && npm run test:browser",
    "example": "node examples/encode.js"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "http://local_proxy@127.0.0.1:44600/git/jburnhams/jpeg-encoder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "canvas": "^3.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  }
}
